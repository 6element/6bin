{
  "name": "6bin",
  "version": "1.0.0",
  "description": "User interface for 6element sensors",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-client": "browserify ./client/main.ts -p [ tsify --noImplicitAny --sourceMap --target ES5 ] -p [minifyify --map false --output /dev/null] --debug > ./client/browserify-bundle.js",
    "watch-client": "watchify ./client/main.ts -p [ tsify --noImplicitAny --sourceMap --target ES5 ]  --debug -o ./client/browserify-bundle.js -v",
    "build-server": "tsc server/main.ts --noImplicitAny --sourceMap --target ES5 --module commonjs",
    "watch-server": "tsc server/main.ts --noImplicitAny --sourceMap --target ES5 --module commonjs -w server/*.ts",
    "start": "docker-compose up server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anthill/6bin.git"
  },
  "keywords": [
    "sensors",
    "user",
    "interface",
    "web",
    "6element",
    "ants"
  ],
  "author": "David Bruant <bruant.d@gmail.com> (https://twitter.com/DavidBruant)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anthill/6bin/issues"
  },
  "homepage": "https://github.com/anthill/6bin",
  "dependencies": {
    "body-parser": "^1.14.1",
    "browserify": "^11.2.0",
    "compression": "^1.6.0",
    "es6-shim": "^0.33.6",
    "express": "^4.13.3",
    "immutable": "^3.7.5",
    "minifyify": "^7.1.0",
    "react": "^0.14.0",
    "react-addons-pure-render-mixin": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.0",
    "redux": "^3.0.3",
    "socket.io": "^1.3.7",
    "socket.io-client": "^1.3.7",
    "tsify": "^0.12.2",
    "typescript": "^1.6.2"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "^1.1.1",
    "babelify": "^6.4.0",
    "chai": "^3.4.0",
    "chai-immutable": "^1.3.0",
    "livereactload": "^2.0.4",
    "mocha": "^2.3.3",
    "react-proxy": "^1.1.1",
    "watchify": "^3.4.0"
  }
}
